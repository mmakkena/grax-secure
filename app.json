{
  "name": "GRAX",
  "description": "GRAX Enterprise : Salesforce Data Lake",
  "repository": "https://github.com/HardingPoint/grax.git",
  "website": "https://grax.io",
  "keywords": ["grax","grax.io","salesforce","elastic","S3"],
  "addons": [
    {
      "plan": "bonsai:shared-30",
      "as": "BONSAI"
    },
    {
      "plan": "engagementgraph:hpt-test",
      "as": "ENGAGEMENTGRAPH"
    },
    {
      "plan": "scheduler:standard",
      "as": "SCHEDULER"
    },
    {
      "plan": "papertrail:choklad",
      "as": "PAPERTRAIL"
    }
  ],
  "env": {
    "GRAX_SFDC_USERNAME":{
      "description": "Salesforce Username : Salesforce user for GRAX",
      "value": ""
    },
    "GRAX_SFDC_PASSWORD":{
      "description": "Salesforce Password : Salesforce password for GRAX",
      "value": ""
    },
    "GRAX_SFDC_TOKEN":{
      "description": "Salesforce Token : Salesforce token for GRAX",
      "value": ""
    },
    "GRAX_SFDC_URL":{
      "description": "Salesforce URL",
      "value": "https://test.salesforce.com"
    },
    "DEBUGGING": {
      "description": "Debugging Flag",
      "value": "true"
    },
    "GRAX_ASYNCH_TIMER":{
      "description": "Asynchronous Timer Interval",
      "value": "10"
    },
    "GRAX_ESMAXBULK":{
      "description": "Elastic Bulk Loading",
      "value": "300"
    },
    "GRAX_S3_OFF":{
      "description": "Turn off S3 Saving, be very careful. Leave ON.",
      "value": "false"
    },
    "GRAX_AUDITTRAIL_OFF":{
      "description": "Leave OFF. Turn off GRAX AuditTrail to Postgres.",
      "value": "true"
    },
    "S3ACCESSKEYID":{
      "description": "S3 Access KeyID",
      "value": ""
    },
    "S3BUCKET":{
      "description": "S3 Access KeyID",
      "value": ""
    },
    "S3REGION":{
      "description": "S3 Access KeyID",
      "value": ""
    },
    "S3SECRETACCESSKEY":{
      "description": "S3 Access KeyID",
      "value": ""
    },
    "GRAX_DESTINATION_SFDC_PASSWORD":{
      "description": "Destination Salesforce Password",
      "value": "",
      "required":false
    },
    "GRAX_DESTINATION_SFDC_TOKEN":{
      "description": "Destination Salesforce Token",
      "value": "",
      "required":false
    },
    "GRAX_DESTINATION_SFDC_URL":{
      "description": "Destination Salesforce Token",
      "value": "https://test.salesforce.com",
      "required":false
    },
    "GRAX_DESTINATION_SFDC_USERNAME":{
      "description": "Destination Salesforce Token",
      "value": "",
      "required":false
    },
    "GRAX_MAX_RESTORE":{
      "description": "Max Number to Bulk Restore",
      "value": "1000",
      "required":false
    },
    "GRAX_OBJECT_TYPES_TO_RESTORE":{
      "description": "Salesforce Object Types to Restore",
      "value": "",
      "required":false
    },
    "GRAX_SYNCHID":{
      "description": "Salesforce Object Name to Restore",
      "value": "GRAX_RESTORE",
      "required":false
    }
  }
}
