{
  "name": "grax",
  "version": "3.2.5",
  "description": "GRAX : Data Lake, Time Machine, and AWS Data Bridge",
  "main": "index.js",
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "scripts": {
    "test": "mocha --exit --timeout 100000",
    "clean": "rm -rf ../grax-secure/*.js",
    "secure": "grunt copy && grunt obfuscator",
    "testsecure": "cd ../grax-secure && npm install && npm run test",
    "documentation": "rm -rf ./docs/*.* && grunt documentation",
    "cover": "istanbul cover _mocha",
    "coveralls": "npm run cover -- --report lcovonly && cat ./coverage/lcov.info | coveralls",
    "build": "npm install && npm audit fix && nsp check && npm run clean && npm run documentation && npm run coveralls && npm run secure && npm run testsecure"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/HardingPoint/grax.git"
  },
  "keywords": [
    "GRAX",
    "Salesforce",
    "Data",
    "Lake"
  ],
  "author": "Harding Point",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/HardingPoint/grax/issues"
  },
  "homepage": "https://github.com/HardingPoint/grax#readme",
  "dependencies": {
    "asynch": "^0.5.2",
    "aws-s3-multipart-copy": "1.0.5",
    "aws-sdk": "2.321.0",
    "bluebird": "3.5.2",
    "body-parser": "^1.18.3",
    "bunyan": "1.8.12",
    "busboy": "0.2.14",
    "coffee-script": "^1.12.7",
    "cookie-parser": "1.4.3",
    "cors": "2.8.4",
    "crypto-js": "^3.1.9-1",
    "csvtojson": "2.0.8",
    "deasync": "^0.1.13",
    "documentation": "^8.1.2",
    "elasticsearch": "^15.1.1",
    "express": "4.16.3",
    "express-basic-auth": "1.1.5",
    "express-jwt": "5.3.1",
    "formidable": "1.2.1",
    "helmet": "3.13.0",
    "http-aws-es": "^6.0.0",
    "https": "^1.0.0",
    "jsforce": "1.9.1",
    "jsonfile": "^4.0.0",
    "jsonwebtoken": "8.3.0",
    "lockfile": "1.0.4",
    "moment": "^2.22.2",
    "morgan": "1.9.1",
    "node-schedule": "1.3.0",
    "path": "0.12.7",
    "path-to-regexp": "2.4.0",
    "pg": "^7.4.3",
    "pg-promise": "8.4.6",
    "pug": "2.0.3",
    "s3-download-stream": "^1.1.1",
    "s3-upload-stream": "^1.0.7",
    "sleep": "5.2.3",
    "unzipper": "0.9.3",
    "uuid": "^3.3.2",
    "xmlbuilder": "10.0.0"
  },
  "devDependencies": {
    "coveralls": "^3.0.2",
    "grunt": "^1.0.3",
    "grunt-contrib-copy": "^1.0.0",
    "grunt-contrib-obfuscator": "^4.0.0",
    "grunt-documentation": "^1.2.3",
    "grunt-mocha": "^1.1.0",
    "istanbul": "^0.4.5",
    "mocha": "^5.2.0",
    "mocha-lcov-reporter": "^1.3.0"
  },
  "engines": {
    "node": "8.3.0"
  }
}
