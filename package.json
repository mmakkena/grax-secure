{
  "name": "grax",
  "version": "3.5.7",
  "description": "GRAX : Data Lake, Time Machine, and AWS Data Bridge",
  "main": "index.js",
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "bin": {
    "grax": "./bin/grax",
    "grax-destination": "./bin/grax-destination",
    "grax-force-restore-receipt": "./bin/grax-force-restore-receipt",
    "grax-meta": "./bin/grax-meta",
    "grax-purge-destination": "./bin/grax-purge-destination"
  },
  "scripts": {
    "dev-build": "npm install && npm audit fix && npm run secure:prep && npm run coveralls && npm run graxreact && npm run secure && npm run secure:test",
    "ci": "nyc npm run ci:test",
    "ci:coveralls:report": "nyc report --reporter=text-lcov | coveralls",
    "ci:test": "NODE_ENV=ci mocha --exit --timeout 200000 lib test",
    "clean": "rm -rf tmp/grax-secure/*.js",
    "documentation": "rm -rf ./docs/*.* && grunt documentation",
    "grax:bulk": "node --max-old-space-size=8192 ./applicationservers/asynch/processor/salesforcebulk.js",
    "grax:bulk:debug": "node --inspect-brk --max-old-space-size=8192 ./applicationservers/asynch/processor/salesforcebulk.js",
    "graxreact": "cd ./applicationservers/web/application/graxreact && npm install && npm run build",
    "secure": "grunt copy && grunt obfuscator",
    "secure:build": "npm run secure:prep && npm install && npm audit fix && npm run graxreact && npm run secure && npm run secure:test:ci && npm run secure:publish",
    "secure:publish": "cd tmp/grax-secure && git config user.email Support@GRAX.io && git config user.name HardingPoint && git add . && git commit -m \"Update based on grax master branch `date`\" && git push origin master",
    "secure:prep": "rm -rf tmp && mkdir tmp && cd tmp && git clone https://$GH_USERNAME:$GH_TOKEN@github.com/HardingPoint/grax-secure.git && cd grax-secure && find . -type f -name '*.js' -exec rm {} +",
    "secure:test": "cd tmp/grax-secure && npm install && npm run test",
    "secure:test:ci": "cd tmp/grax-secure && npm install && npm run ci:test",
    "test:coveralls": "nyc npm run test:all && nyc report --reporter=text-lcov | coveralls",
    "heroku-postbuild": "echo Skip build on Heroku",
    "test": "NODE_ENV=test mocha --exit --timeout 200000",
    "test:all": "NODE_ENV=test mocha --exit --timeout 200000 lib test",
    "test:debug": "NODE_ENV=test mocha --inspect-brk"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/HardingPoint/grax.git"
  },
  "keywords": [
    "GRAX",
    "Salesforce",
    "Data",
    "Lake"
  ],
  "author": "Harding Point",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/HardingPoint/grax/issues"
  },
  "homepage": "https://github.com/HardingPoint/grax#readme",
  "dependencies": {
    "@azure/storage-blob": "^10.3.0",
    "@google-cloud/storage": "^2.5.0",
    "ajv-keywords": "^3.2.0",
    "asynch": "^0.5.2",
    "atob": "2.1.2",
    "aws-s3-multipart-copy": "1.0.5",
    "aws-sdk": "2.321.0",
    "bluebird": "3.5.2",
    "body-parser": "^1.18.3",
    "btoa": "1.2.1",
    "bunyan": "1.8.12",
    "busboy": "0.2.14",
    "chalk": "^2.4.1",
    "cli-spinners": "^1.3.1",
    "coffee-script": "^1.12.7",
    "commander": "^2.19.0",
    "config": "^3.0.1",
    "cookie-parser": "1.4.3",
    "cors": "2.8.4",
    "crypto-js": "^3.1.9-1",
    "csvtojson": "2.0.8",
    "deasync": "^0.1.13",
    "documentation": "^8.1.2",
    "elasticsearch": "^15.1.1",
    "eventemitter2": "^5.0.1",
    "expect": "^23.6.0",
    "express": "4.16.3",
    "express-basic-auth": "^1.2.0",
    "express-jwt": "5.3.1",
    "formidable": "1.2.1",
    "helmet": "3.13.0",
    "http-aws-es": "^6.0.0",
    "https": "^1.0.0",
    "inquirer": "^6.2.1",
    "inquirer-autocomplete-prompt": "^1.0.1",
    "jsforce": "1.9.1",
    "jsonfile": "^4.0.0",
    "jsonwebtoken": "8.3.0",
    "lockfile": "1.0.4",
    "lodash": "^4.17.11",
    "moment": "^2.22.2",
    "morgan": "1.9.1",
    "node-schedule": "1.3.0",
    "nodemailer": "^5.1.1",
    "ora": "^3.0.0",
    "path": "0.12.7",
    "path-to-regexp": "2.4.0",
    "pg": "^7.4.3",
    "pg-promise": "8.4.6",
    "pug": "2.0.3",
    "readline": "1.3.0",
    "request": "^2.88.0",
    "s3-download-stream": "^1.1.1",
    "s3-upload-stream": "^1.0.7",
    "sleep": "5.2.3",
    "ssh2": "0.8.2",
    "unzipper": "0.9.3",
    "uuid": "^3.3.2",
    "winston": "^3.1.0",
    "xmlbuilder": "10.0.0"
  },
  "devDependencies": {
    "config": "^3.0.1",
    "copy-paste": "^1.3.0",
    "coveralls": "^3.0.2",
    "dotenv": "^6.2.0",
    "eslint": "5.6.1",
    "grunt": "^1.0.4",
    "grunt-contrib-copy": "^1.0.0",
    "grunt-contrib-obfuscator": "^4.0.0",
    "grunt-documentation": "^1.2.3",
    "grunt-mocha": "^1.2.0",
    "istanbul": "^0.4.5",
    "javascript-obfuscator": "^0.18.1",
    "jest": "^23.6.0",
    "jest-mock": "^23.2.0",
    "mocha": "^6.1.4",
    "mocha-lcov-reporter": "^1.3.0",
    "ndb": "^1.0.42",
    "nyc": "^13.3.0",
    "sinon": "^7.2.7",
    "supertest": "^3.4.2"
  },
  "engines": {
    "node": "8.3.0"
  },
  "heroku-run-build-script": true
}
